{
  "name": "example-atom",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/components/atoms/example-atom/src",
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/components/atoms/example-atom/src/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "build": {
      "executor": "@nx/js:tsc",
      "options": {
        "outputPath": "libs/components/atoms/example-atom/dist",
        "main": "libs/components/atoms/example-atom/src/index.ts",
        "tsConfig": "libs/components/atoms/example-atom/tsconfig.lib.json",
        "assets": ["libs/components/atoms/example-atom/*.md"],
        "generateExportsField": true,
        "clean": true
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx vitest libs/components/atoms/example-atom/src/**/*.spec.tsx --globals --run --environment=jsdom"
      }
    }
  }
}
